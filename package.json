{
    "name": "canva-bot-serverless",
    "version": "1.0.0",
    "description": "Telegram Bot for Canva Automation on Vercel",
    "main": "api/webhook.ts",
    "scripts": {
        "build": "tsc",
        "dev": "vercel dev",
        "start": "ts-node src/local.ts",
        "dev:local": "ts-node src/local.ts",
        "deploy": "vercel deploy --prod",
        "migrate": "ts-node scripts/migrate.ts",
        "get-cookie": "ts-node scripts/get_cookie.ts",
        "auto-kick": "ts-node scripts/auto_kick.ts",
        "process-queue": "ts-node scripts/process_queue.ts",
        "set-webhook": "ts-node scripts/set_webhook.ts",
        "revoke-stale": "ts-node scripts/revoke_stale.ts",
        "sync-members": "ts-node scripts/sync_member_count.ts",
        "maintenance": "npm run revoke-stale && npm run process-queue && npm run sync-members"
    },
    "keywords": [
        "telegram",
        "bot",
        "canva",
        "serverless",
        "vercel"
    ],
    "author": "Antigravity",
    "license": "ISC",
    "dependencies": {
        "@libsql/client": "^0.14.0",
        "axios": "^1.7.9",
        "dotenv": "^16.4.5",
        "grammy": "^1.33.0",
        "puppeteer-core": "^24.34.0",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2",
        "semicolon": "^0.1.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2"
    },
    "devDependencies": {
        "@types/node": "^22.10.1",
        "@vercel/node": "^3.2.0",
        "vercel": "^39.2.1"
    }
}